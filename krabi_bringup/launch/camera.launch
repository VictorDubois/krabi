<launch>
    <!--<arg name="hostname" default="192.168.20.16"/>
    <node name="tim_loc" pkg="sick_scan" type="sick_generic_caller" respawn="false" output="screen">
        <param name="sw_pll_only_publish" type="bool" value="true"/>
	<remap from="scan" to="scan_loc"/>
